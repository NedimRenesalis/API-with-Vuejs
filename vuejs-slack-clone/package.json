{
  "name": "vuejs-slack-clone",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:server": "nodemon ./bin/www",
    "dev:client": "cd client && npm run serve",
    "dev": "concurrently --kill-others-on-fail 'npm run dev:server' 'npm run dev:client'",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^9.0.2",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "nexmo-client": "^8.0.3",
    "postcss": "^8.2.15"
  },
  "devDependencies": {
    "concurrently": "^6.1.0",
    "nodemon": "^2.0.7"
  }
}
